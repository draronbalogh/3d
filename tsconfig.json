{
  "compilerOptions": {
    "target": "es2020" /*es5*/ /*changed later*/,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "checkJs": true /*added later*/,
    "jsx": "react-jsx",
    "baseUrl": "./" /*added later*/,
    "paths": {
      "@/*": ["./*"] /*added later*/
    }
  },
  "include": [".eslintrc.js", "src", "./babel.config.js", "./jest.config.js"] /*added later*/
}
